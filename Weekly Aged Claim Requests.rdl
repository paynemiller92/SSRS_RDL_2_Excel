          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="ClaimOwnerName">
          <DataField>ClaimOwnerName</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="ClaimStatus">
