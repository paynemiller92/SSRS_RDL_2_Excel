  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SQL_AGILESQL2</DataSourceName>
        <CommandText>select convert(varchar (10),DATEDIFF(day, a.created_date,(SELECT cast(dateadd(day,1-datepart(dw, getdate()), getdate() + 1) as date)))) as AgeofClaim
, convert(varchar (10),a.Created_Date) as CreatedDate
, (select top 1 b.DISPLAY_NAME from QDM.ABL_QDM_USERLIST b where b.USERNAME = a.OWNER) as ClaimOwnerName
,'Open' as ClaimStatus
, a.Claim_Number
from QDM.ABL_QDM_FIELDCLAIM a 
	--left join QDM.ABL_QDM_USERLIST b on RTRIM(a.owner) = rtrim(b.username)
where a.created_date between '2014-02-04' and (SELECT cast(dateadd(day,1-datepart(dw, getdate()), getdate() + 1) as date))
and status_code = 000001
order by ClaimOwnerName asc</CommandText>
        <UseGenericDesigner>true</UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="AgeofClaim">
          <DataField>AgeofClaim</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="CreatedDate">
